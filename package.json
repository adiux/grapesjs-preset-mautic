{
  "name": "grapesjs-preset-mautic",
  "version": "1.0.12",
  "description": "Grapesjs Preset Mautic maintained by aivie.ch",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/adiux/grapesjs-preset-mautic.git"
  },
  "scripts": {
    "deploy": "npm run build && npm publish && git add . && git commit && git push",
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "v:minor": "npm version --no-git-tag-version minor",
    "build-dev": "npm run && webpack --env production",
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build"
  },
  "keywords": [
    "grapesjs",
    "plugin"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-loader": "^9.1.3",
    "grapesjs-cli": "^4.1.3",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "airbnb/base",
      "plugin:prettier/recommended"
    ],
    "ignorePatterns": [
      "src/api/mautic"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "license": "MIT",
  "dependencies": {
    "mjml": "^4.9.3",
    "mjml-browser": "^4.14.1"
  },
  "peerDependencies": {
    "grapesjs": "^0.21.7"
  }
}
